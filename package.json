{
  "name": "etm12-pixel-app",
  "homepage": "https://etm12.dev",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "daggy": "^1.4.0",
    "file-saver": "^2.0.2",
    "infestines": "^0.4.11",
    "karet": "^4.1.0",
    "karet.util": "^0.19.1",
    "kefir": "^3.8.6",
    "kefir.partial.lenses": "^14.17.0",
    "kefir.partial.lenses.history": "^1.2.0",
    "kefir.ramda": "^0.26.0",
    "normalize.css": "^8.0.1",
    "partial.lenses": "^14.17.0",
    "partial.lenses.validation": "^2.0.0",
    "ramda": "^0.26.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.2",
    "yiq": "^3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "npm run test:cov && codecov",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:simple": "npm run test -- --watchAll=false",
    "test:cov": "npm run test -- --watchAll=false --collectCoverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "additional-rule": "warn",
      "no-unused-vars": [
        1,
        {
          "varsIgnorePattern": "[K|T|Model]"
        }
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:simple"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/tests/**",
      "!src/test-utils.js",
      "!src/typedef.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/kefir": "^3.8.2",
    "@types/ramda": "^0.26.25",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-router": "^5.1.1",
    "@types/react-router-dom": "^5.1.0",
    "codecov": "^3.6.1",
    "cz-conventional-changelog": "3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.0",
    "enzyme-to-json": "^3.4.2",
    "husky": "^3.0.9",
    "jest-canvas-mock": "^2.1.2",
    "jsverify": "^0.8.4",
    "node-sass": "^4.12.0",
    "react-test-renderer": "^16.10.2",
    "ts-toolbelt": "^4.7.16",
    "typescript": "^3.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
